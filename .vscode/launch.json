{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${fileDirname}",
      "envFile": "${workspaceFolder}/conf/config.yaml",
      "env": {
        "CONFIG_FILE": "${workspaceFolder}/conf/config.yaml"
      },
      "args": ["${workspaceFolder}/conf/config.yaml"],
      "showLog": true,
      "trace": "verbose",
    }
  ]
}
